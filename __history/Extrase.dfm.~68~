  object DS: TDataSource
    DataSet = Q
    Left = 40
    Top = 48
  end
  object Q: TQuery
    DatabaseName = 'dsTest'
    ParamCheck = False
    SQL.Strings = (
      
        'SELECT COD_FISCAL, DENFUR, DATA, NROP, FACTURA, DATAFC, SUMADB, ' +
        'SUMACR, SUMA, "NU" VALIDAT, CODFUR FROM (SELECT E.*, CODFUR FROM' +
        ' EXTRASE E LEFT JOIN FUR F ON (E.COD_FISCAL = F.COD_FISCAL) OR (' +
        'CONCAT("RO",E.COD_FISCAL) = F.COD_FISCAL) OR (E.COD_FISCAL = CON' +
        'CAT("RO",F.COD_FISCAL))) EXT LEFT JOIN FRES0910 F ON EXT.CODFUR ' +
        '= F.CODFC')
    Left = 8
    Top = 48
  end
  object QF: TQuery
