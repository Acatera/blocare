  object DS: TDataSource
    DataSet = Query
    Left = 48
    Top = 80
  end
  object Query: TQuery
    Active = True
    DatabaseName = 'dsTest'
    SQL.Strings = (
      'SELECT'
      
        'INFO3, CONCAT(RIGHT(DATA, 2), '#39'.'#39', MID(DATA, 5, 2), '#39','#39', LEFT(DA' +
        'TA, 4)) DATA, CONCAT(LEFT(TIMP, 2), '#39':'#39', MID(TIMP, 3, 2), '#39':'#39', R' +
        'IGHT(TIMP, 2)) TIMP, F.DENFUR, STATIA, USER, MID(G.DESCRIERE,1,1' +
        '00) DESCRIERE, CONCAT(IF(ORD(TIP2)&1=1, "Telefonic", ""), IF(ORD' +
        '(TIP2)&2=2, "LogMeIn", ""), IF(ORD(TIP2)&4=4, "Client", ""), IF(' +
        'ORD(TIP2)&8=8, "Deplasare","")) AS TIP, CONCAT(IF(ORD(TIP2)&16=1' +
        '6, "Scurta", ""), IF(ORD(TIP2)&32=32, "Medie", ""), IF(ORD(TIP2)' +
        '&128=128, "Lunga", ""), IF(ORD(TIP2)&256=256, "Foarte lunga","")' +
        ') AS DURATA '
      
        'FROM `fdsoft`.`LOG` L LEFT JOIN `fdsoft`.`GENERAL` G ON G.GRUPA=' +
        #39'JUR'#39' AND L.INFO3=G.COD LEFT JOIN `fdsoft`.`FUR` F ON L.INFO1=F.' +
        'CODFUR '
      'WHERE L.TIP='#39'J'#39)
    Left = 16
    Top = 80
  end
end
