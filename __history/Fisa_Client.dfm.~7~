  object QAux1: TQuery
    DatabaseName = 'dsTest'
    ParamCheck = False
    SQL.Strings = (
      
        'SELECT "SOLD precedent:" AS EXPLICN, " " AS CONT_COR, " " AS DEB' +
        'IT, " " AS CREDIT, 0 AS SOLD, 0 AS FACT, 0 AS DB, 0 AS CR, " " A' +
        'S DATAD UNION ALL SELECT CONCAT("Fact. ", NRDFC," / ",CONCAT(SUB' +
        'STRING(DATAFC,1,2),".",SUBSTRING(DATAFC,3,2),".",SUBSTRING(DATAF' +
        'C,5,4))," - ",SUBSTRING(EXPLIC,1,1),LOWER(SUBSTRING(EXPLIC,2))) ' +
        'AS EXPLIC, CONTCOR AS CONT_COR, IF(SUMADB-SUMACR > 0, ROUND(SUMA' +
        'DB-SUMACR, 2), 0) AS DEBIT, IF(SUMADB-SUMACR < 0, ROUND(SUMADB-S' +
        'UMACR, 2), 0) AS CREDIT, 0 AS SOLD, (SUMADB-SUMACR) AS FACT, 0  ' +
        'AS DB, 0 AS CR, DATAFC AS DATAD FROM `quality`.`FRES1208` WHERE ' +
        '(CODFC = "R1834799") AND (CONTFC="4111") UNION ALL SELECT "Sold ' +
        'la sfarsitul lunii precedente:" AS EXPLICN, " " AS CONT_COR, " "' +
        ' AS DEBIT, " " AS CREDIT, 0 AS SOLD, 0 AS FACT, 0  AS DB, 0 AS C' +
        'R,  " " AS DATAD')
    Left = 32
    Top = 64
  end
  object QAux2: TQuery
    DatabaseName = 'dsTest'
    ParamCheck = False
    SQL.Strings = (
      
        'SELECT CONCAT("Fact. ", NRDFC," / ",CONCAT(SUBSTRING(DATAFC,1,2)' +
        ',".",SUBSTRING(DATAFC,3,2),".",SUBSTRING(DATAFC,5,4))," - ",SUBS' +
        'TRING(EXPLIC,1,1),LOWER(SUBSTRING(EXPLIC,2))) AS EXPLIC, CONTCOR' +
        ' AS CONT_COR, IF(SUMADB-SUMACR > 0, ROUND(SUMADB-SUMACR, 2), 0) ' +
        'AS DEBIT, IF(SUMADB-SUMACR < 0, ROUND(SUMADB-SUMACR, 2), 0) AS C' +
        'REDIT, 0 AS SOLD, (SUMADB-SUMACR) AS FACT, 0 AS DB, 0 AS CR, DAT' +
        'AFC AS DATAD FROM `quality`.`FRES0310` WHERE (CODFC = "R1834799"' +
        ') AND (CONTFC="4111") AND (SUMADB-SUMACR <> 0)')
    Left = 64
    Top = 64
  end
