  object Query: TQuery
    DatabaseName = 'dsTest'
    ParamCheck = False
    SQL.Strings = (
      
        'SELECT EXPLICN, CONT_COR, DEBIT, CREDIT, 0 AS SOLD, FACT, 0  AS ' +
        'DB, 0 AS CR FROM (SELECT "SOLD precedent:" AS EXPLICN, " " AS CO' +
        'NT_COR, " " AS DEBIT, " " AS CREDIT, 0 AS FACT, " " AS DATAD UNI' +
        'ON ALL SELECT CONCAT(IF(LEFT(DEBIT, 3)="512","Ord. ", ""), NRDI,' +
        '" / ",CONCAT(SUBSTRING(DATAI,1,2),".",SUBSTRING(DATAI,3,2),".",S' +
        'UBSTRING(DATAI,5,4))," - ",SUBSTRING(EXPLICN,1,1),LOWER(SUBSTRIN' +
        'G(EXPLICN,2))) AS EXPLIC, DEBIT AS CONT_COR, " " AS DEBIT, SUM(S' +
        'UMA) AS CREDIT, CONCAT("-",SUM(SUMA)) AS FACT, DATAI AS DATAD FR' +
        'OM `quality`.`CM0110` WHERE (FURCLI = "RO1806470") AND (CREDIT="' +
        '4111") GROUP BY NRDI, FURCLI UNION ALL SELECT CONCAT("Fact. ", N' +
        'UMAR," / ",CONCAT(SUBSTRING(DATA,1,2),".",SUBSTRING(DATA,3,2),".' +
        '",SUBSTRING(DATA,5,4))," - ",SUBSTRING(DENUMIRE,1,1),LOWER(SUBST' +
        'RING(DENUMIRE,2))) AS EXPLIC, CONT AS CONT_COR, ROUND(SUM(VALOAR' +
        'E+VAL_TVA),2) AS DEBIT, " " AS CREDIT, ROUND(SUM(VALOARE+VAL_TVA' +
        '),2) AS FACT, DATA AS DATAD FROM `quality`.`MIV0110` WHERE (CODF' +
        'UR = "RO1806470") AND (CONT_COR="4111") AND (ANULAT <> "A") GROU' +
        'P BY NUMAR, CODFUR UNION ALL SELECT CONCAT(IF(LEFT(DEBIT, 3)="51' +
        '2","Ord. ", ""), NRDI," / ",CONCAT(SUBSTRING(DATAI,1,2),".",SUBS' +
        'TRING(DATAI,3,2),".",SUBSTRING(DATAI,5,4))," - ",SUBSTRING(EXPLI' +
        'CN,1,1),LOWER(SUBSTRING(EXPLICN,2))) AS EXPLIC, DEBIT AS CONT_CO' +
        'R, " " AS DEBIT, SUM(SUMA) AS CREDIT, CONCAT("-",SUM(SUMA)) AS F' +
        'ACT, DATAI AS DATAD FROM `quality`.`CM0210` WHERE (FURCLI = "RO1' +
        '806470") AND (CREDIT="4111") GROUP BY NRDI, FURCLI UNION ALL SEL' +
        'ECT CONCAT("Fact. ", NUMAR," / ",CONCAT(SUBSTRING(DATA,1,2),".",' +
        'SUBSTRING(DATA,3,2),".",SUBSTRING(DATA,5,4))," - ",SUBSTRING(DEN' +
        'UMIRE,1,1),LOWER(SUBSTRING(DENUMIRE,2))) AS EXPLIC, CONT AS CONT' +
        '_COR, ROUND(SUM(VALOARE+VAL_TVA),2) AS DEBIT, " " AS CREDIT, ROU' +
        'ND(SUM(VALOARE+VAL_TVA),2) AS FACT, DATA AS DATAD FROM `quality`' +
        '.`MIV0210` WHERE (CODFUR = "RO1806470") AND (CONT_COR="4111") AN' +
        'D (ANULAT <> "A") GROUP BY NUMAR, CODFUR ORDER BY CONCAT(SUBSTRI' +
        'NG(DATAD,5,4),SUBSTRING(DATAD,3,2),SUBSTRING(DATAD,1,2))) AS TEM' +
        'P')
    Top = 64
  end
